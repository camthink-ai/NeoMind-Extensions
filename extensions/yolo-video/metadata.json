      "sha256": "",
      "size": 0
    },
    "linux-x86_64": {
      "url": "https://github.com/camthink-ai/NeoMind-Extensions/releases/download/v1.0.0/libneomind_extension_yolo_video-linux-x86_64.so",
      "sha256": "",
      "size": 0
    },
    "windows-x86_64": {
      "url": "https://github.com/camthink-ai/NeoMind-Extensions/releases/download/v1.0.0/neomind_extension_yolo_video-windows-x86_64.dll",
      "sha256": "",
      "size": 0
    }
  },
  "requirements": {
    "min_neomind_version": "0.5.8",
    "network": false,
    "api_keys": []
  },
  "permissions": [],
  "safety": {
    "timeout_seconds": 60,
    "max_memory_mb": 500
  }
}
